<h1>患者様一覧</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_tag users_path, :method => 'get' do %> 
      <p>
        <%= text_field_tag :search, params[:search], id: 'search' %>
        <%= submit_tag "検索", :name => nil %>
      </p>
    <% end %>
  </div>
</div>

<ul class="staffs">
   <%= render @users %>
</ul>
